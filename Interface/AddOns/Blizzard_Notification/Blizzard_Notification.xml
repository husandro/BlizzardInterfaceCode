<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Frame name="NotificationIconFrameTemplate" virtual="true">
		<Size x="25" y="25"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="NotificationIcon" atlas="plunderstorm-new-dot-lg">
					<Size x="25" y="25"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>

	<Frame name="LargeNotificationIconFrameTemplate" virtual="true">
		<Size x="30" y="30"/>
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="NotificationIcon" atlas="plunderstorm-new-dot-lg">
					<Size x="30" y="30"/>
					<Anchors>
						<Anchor point="CENTER"/>
					</Anchors>
				</Texture>
			</Layer>
		</Layers>
	</Frame>
</Ui>
