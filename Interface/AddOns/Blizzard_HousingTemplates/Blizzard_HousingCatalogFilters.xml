<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\Blizzard_SharedXML\UI.xsd">
	<Frame name="HousingCatalogFiltersTemplate" mixin="HousingCatalogFiltersMixin" virtual="true">
		<Frames>
			<DropdownButton parentKey="FilterDropdown" inherits="WowStyle1FilterDropdownTemplate" setAllPoints="true">
				<KeyValues>
					<KeyValue key="resizeToText" type="boolean" value="false"/>
				</KeyValues>
			</DropdownButton>
		</Frames>
	</Frame>

	<EditBox name="HousingCatalogSearchBoxTemplate" mixin="HousingCatalogSearchBoxMixin" inherits="SearchBoxTemplate" letters="40" virtual="true">
		<Scripts>
			<OnLoad method="OnLoad"/>
			<OnTextChanged method="OnTextChanged"/>
		</Scripts>
	</EditBox>
</Ui>
